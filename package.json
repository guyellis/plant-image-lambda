{
  "author": "Guy Ellis <wildfiction@gmail.com>",
  "bugs": {
    "url": "https://github.com/guyellis/plant-image-lambda/issues"
  },
  "dependencies": {
    "async": "2.6.1",
    "aws-sdk": "2.247.1",
    "gm": "1.23.1"
  },
  "description": "Image Lambda for Plant Project",
  "devDependencies": {
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jest": "21.15.3",
    "eslint-plugin-security": "1.4.0",
    "jest": "22.4.3",
    "pre-commit": "1.2.2"
  },
  "engines": {
    "node": "8.11.2",
    "npm": "5.6.0"
  },
  "homepage": "https://github.com/guyellis/plant-image-lambda#readme",
  "license": "MIT",
  "main": "index.js",
  "name": "plant-image-lambda",
  "pre-commit": {
    "colors": true,
    "run": [
      "lint",
      "coverage"
    ],
    "silent": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guyellis/plant-image-lambda.git"
  },
  "scripts": {
    "build": "make lambda",
    "clean": "make clean",
    "coverage": "jest --coverage",
    "lint": "eslint --ext .js . --max-warnings 0",
    "lintfix": "eslint --ext .js . --fix",
    "test": "jest",
    "upprod": "LAMBDA_FUNCTION_NAME=iPlant PLANT_IMAGE_HOST=$PLANT_IMAGE_HOST_PROD PLANT_IMAGE_PORT=$PLANT_IMAGE_PORT_PROD make uploadlambda",
    "uptest": "LAMBDA_FUNCTION_NAME=iPlantTest PLANT_IMAGE_HOST=$PLANT_IMAGE_HOST_TEST PLANT_IMAGE_PORT=$PLANT_IMAGE_PORT_TEST make uploadlambda"
  },
  "version": "2.0.0"
}
